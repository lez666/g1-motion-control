<mujoco model="largebox">
    <compiler meshdir="./"/>

    <default>
        <geom friction="0.9 0.5 0.5" rgba="0.7 0.8 0.9 0.7"/>
        <joint damping="0.5"/>
    </default>

    <asset>
        <mesh name="largebox_mesh" file="largebox.obj" scale="1 1 1"/>
    </asset>

    <worldbody>
        <body name="largebox_link">
            <inertial pos="0 0 0" mass="0.1" diaginertia="0.002 0.002 0.002"/>
            <geom type="mesh" mesh="largebox_mesh"
                  pos="0 0 0" quat="1 0 0 0"
                  rgba="0.7 0.8 0.9 0.7"
                  friction="0.9 0.5 0.5"
                  solref="0.02 1"
                  solimp="0.9 0.95 0.001"/>
        </body>
    </worldbody>
</mujoco>
